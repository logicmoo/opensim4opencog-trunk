<?xml version="1.0" encoding="ISO-8859-1"?>
<aiml>	
  <category>
    <pattern>RELOAD AIML</pattern>
    <template>
        <system>aiml reload</system>
    </template>
  </category>
  <category>
    <pattern>evraj</pattern>
    <template>
      <system>evinfo rae</system>
    </template>
  </category>
  
  <!-- Sitting in secondlife -->
  <category>
    <pattern>SIT _</pattern>
    <template>
      <think>
        <system>sit</system>
      </think>In <system>locate</system> am sitting now.
    </template>
  </category>
  <category>
    <pattern>SIT ON *</pattern>
    <template>
      <think>
        <system>
          sit on <star/>
        </system>
      </think>
      I am sitting on <star/> with location <system>locate</system>
    </template>
  </category>

  <!-- Teleport in secondlife -->
  <category>
    <pattern>TELEPORT HERE</pattern>
    <template>
        Teleporting to <system>
          eval (thisClient.WorldSystem.GetObject "<id/> 1")
        </system> <system>locate <id/></system>
      <think>
        <system>
          say thread teleport 
          <system>     KEYWORDS
            (enqueue (.TeleportTo thisClient.WorldSystem.TheSimAvatar  (thisClient.WorldSystem.GetObject "<id/> 1")))
          </system>
      </system>
      </think>
      </template>
  </category>

  <!-- Sitting in secondlife -->
  <category>
    <pattern>FOLLOW NO ONE *</pattern>
    <template>
      <think>
        <system>stop-following</system>
      </think>Ok, I am on my own now.
    </template>
  </category>
  <category>
    <pattern>STOP FOLLOWING *</pattern>
    <template>
      <think>
        <system>stop-following</system>
      </think>Ok, I am on my own now.
    </template>
  </category>
  <category>
    <pattern>WHAT ARE YOU DOING</pattern>
    <template>
      <system lang="lisp">thisClient.WorldSystem.TheSimAvatar.CurrentAction</system>
    </template>
  </category>
  <category>
    <pattern>
      SIT ON <optional>THE</optional> *
    </pattern>
    <template>
      <system>
        sit on "<star/>" 1
      </system>
    </template>
  </category>

  <!-- simbot command -->
  <category>
    <pattern>WANDER ARROUND</pattern>
    <template>
      <think>
        <system>simbot start</system>
      </think>Ok, I am on my own now wandering.
    </template>
  </category>

  <!-- dotlisp command -->
  <category>
    <pattern>DOTLISP *</pattern>
    <template>
      <system lang="lisp">
        <star/>
      </system>
    </template>
  </category>

  <!-- exec command -->
  <category>
    <pattern>EXEC *</pattern>
    <template>
      result of "<star/>" is: <system lang="bot">
        <star/>
      </system>
    </template>
  </category>

  <category>
    <pattern>
      <bot>name</bot> EXEC *
    </pattern>
    <template>
      result of "<star/>" is: <system lang="bot">
        <star/>
      </system>
    </template>
  </category>
</aiml>

